<%
  my $rep_home = app->root;;
  my $users = [qw/kimoto ken/];
%>

% layout 'common';
  %= include '/include/header', title => 'Gitprep';
  <table class="project_list">
    <th>User</th>
    % my $toggle = 0;
    % for my $user (@$users) {
      <tr class="<%= $toggle++ ? 'light' : 'dark' %>">
        <td>
          <a class="list" href="<%= url_for("/$user") %>">
            <%= $user %>
          </a>
        </td>
    % }
  </table>
